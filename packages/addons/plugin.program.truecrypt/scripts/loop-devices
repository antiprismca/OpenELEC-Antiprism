[ -e /dev/loop1 ] || mknod /dev/loop1 b 7 1
[ -e /dev/loop2 ] || mknod /dev/loop2 b 7 2
[ -e /dev/loop3 ] || mknod /dev/loop3 b 7 3
[ -e /dev/loop4 ] || mknod /dev/loop4 b 7 4
[ -e /dev/loop5 ] || mknod /dev/loop5 b 7 5
[ -e /dev/loop6 ] || mknod /dev/loop6 b 7 6
[ -e /dev/loop7 ] || mknod /dev/loop7 b 7 7
[ -e /dev/loop8 ] || mknod /dev/loop8 b 7 8
[ -e /dev/loop9 ] || mknod /dev/loop9 b 7 9
[ -e /dev/loop10 ] || mknod /dev/loop10 b 7 10 


